<% layout("./layout/boilerplate") %>
<h1>Similar Questions</h1>
<ul>
    <% if (result.length > 0) { %>
        <% result.forEach(item => { %>
            <li>
                
                <strong>Question:</strong> <%= item.question %>
            </li>
        <% }) %>
    <% } else { %>
        <li>No results found</li>
    <% } %>
</ul>